        "flowdock",
        "chat",
        "bot",
        "butler"
    ],
    "author": "Daniel Gempesaw <gempesaw@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gempesaw/carebot/issues"
    },
    "homepage": "https://github.com/gempesaw/carebot#readme",
    "dependencies": {
        "flowdock": "^0.9.0"
    },
    "devDependencies": {
        "babel-cli": "^6.6.5",
        "babel-preset-es2015": "^6.6.0",
        "jasmine-core": "^2.4.1",
        "karma": "^0.13.21",
        "karma-chrome-launcher": "^0.2.2",
        "karma-jasmine": "^0.3.7",
        "nodemon": "^1.9.1"
    }
