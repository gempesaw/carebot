{
    "name": "carebot",
    "version": "0.0.1",
    "description": "sc flowdock bot",
    "main": "index.js",
    "scripts": {
        "start": "node_modules/.bin/nodemon lib/index.js --exec babel-node",
        "build": "node_modules/.bin/babel lib -d dist",
        "test": "node_modules/.bin/karma start karma.conf.js --auto-watch --no-color",
